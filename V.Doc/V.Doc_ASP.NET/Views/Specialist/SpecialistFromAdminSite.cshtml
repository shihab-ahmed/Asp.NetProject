@model IEnumerable<V.Doc_ASP.NET.Models.CustomModel.SpecialistModel>

@{
    ViewBag.Title = "SpecialistFromAdminSite";
    Layout = "~/Views/Shared/Admin_MainLayout.cshtml";
}
<h2>SpecialistFromAdminSite</h2>

<p>
    @Html.ActionLink("Create New", "CreateSpecialist")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Symptom)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Symptom)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit",new { id=item.Id }) |
            @Html.ActionLink("Delete", "DeleteSpecialist", new { id=item.Id })
        </td>
    </tr>
}

</table>
